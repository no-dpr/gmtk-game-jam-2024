; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BusyBones"
run/main_scene="res://ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.png"

[autoload]

Resources="*res://autoload/resources.tscn"
NavigationGlobals="*res://autoload/navigation_globals.tscn"
Globals="*res://autoload/Globals.tscn"
PhaseManager="*res://autoload/phase_manager.gd"
MusicManager="*res://autoload/music_manager.gd"

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="untitled necromancy game"

[global_group]

entities=""
evil_guys=""

[input]

select={
"deadzone": 0.5,
"events": []
}
interact={
"deadzone": 0.5,
"events": []
}

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/defaults/default_clear_color=Color(0.223529, 0.196078, 0.219608, 1)
